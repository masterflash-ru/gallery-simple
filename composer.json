{
    "name": "masterflash-ru/gallery-simple",
    "description": "Простейшая фотогалерея",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "simba"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "zendframework/zend-cache": "^2.7",
        "masterflash-ru/ado": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mf\\GallerySimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\GallerySimple\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.0-dev"
        },
        "zf": {
            "component": "Mf\\GallerySimple",
            "config-provider": "masterflash-ru\\Mf\\GallerySimple\\ConfigProvider"
        }
    }
}
